{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "no-console": {
      "severity": "warning"
    },
    "no-debugger": {
      "severity": "warning"
    },
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}
